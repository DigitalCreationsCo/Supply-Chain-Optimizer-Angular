{
  "name": "supply-chain-optimizer",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --configuration production --aot --build-optimizer --vendor-chunk --common-chunk --delete-output-path",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/supply-chain-optimizer/stats.json",
    "watch": "ng build --watch --configuration development",
